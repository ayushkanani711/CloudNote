{
  "version": 2,
  "builds": [
    {"src": "app.js","use": "@vercel/node"},
    {"src": "/routes/**/*.js","use": "@vercel/node"}
  ],
  "routes": [
    {"src": "/api/auth/.*", "dest": "/routes/auth.js" },
    {"src": "/api/notes/.*", "dest": "/routes/notes.js" },
    {"src": "/(.*)","dest": "/app.js"}
  ]
}
